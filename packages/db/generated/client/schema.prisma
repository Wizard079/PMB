generator client {
  provider = "prisma-client-js"
  output   = "../generated/client"
}

datasource db {
  provider = "sqlite"
  url      = "file:/home/badboy/myEnv/tmp/PMB/packages/db/prisma/dev.db"
}

enum MatchStatus {
  NOT_STARTED
  IN_PROGRESS
  COMPLETED
}

model Matches {
  id       Int         @id @default(autoincrement())
  team1    String
  team2    String
  runs1    Int
  balls1   Int
  wickets1 Int
  runs2    Int
  balls2   Int
  wickets2 Int
  status   MatchStatus
}

model Message {
  id             Int    @id @default(autoincrement())
  topic          String
  messageContent String
}
