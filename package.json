{
  "name": "monorepo",
  "private": true,
  "scripts": {
    "db:generate": "cd packages/db && prisma generate && cd ../..",
    "build": "npm run db:generate && turbo build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "dev": "turbo run dev"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.0",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "prisma": "^6.6.0"
  }
}
